{
    "manifest_version": 3,
    "name": "Clean New Tab",
    "version": "1.0.0",
    "description": "A minimal, customizable new tab page with quick links and beautiful backgrounds",
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}